[user]
	name = Doug Ilijev
	email = doug.ilijev@utexas.edu
[alias]
	cfg = config --global
	cfl = config -l
	cfgl = config --global -l
	undo = reset --hard HEAD
	st = status
	ci = commit
	cm = commit -m
	co = checkout
	br = branch
	ss = status -s
	glog = log --graph
	lg = log --pretty=format:\"[%h]  %ad  (%an <%ae>)%n    %s%n\"
	gl = log --graph --pretty=format:\"[%h]  %ad  (%an <%ae>)%n    %s%n\"
	dc = diff --cached
[pretty]
	compact = [%h]  %ad  (%an <%ae>)%n    %s%n
[format]
	pretty = medium
